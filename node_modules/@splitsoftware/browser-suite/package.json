{
  "name": "@splitsoftware/browser-suite",
  "version": "1.1.2",
  "description": "Split Suite for JavaScript on Browser",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "README.md",
    "CHANGES.txt",
    "cjs",
    "esm",
    "src",
    "types"
  ],
  "scripts": {
    "check": "npm run check:lint && npm run check:types",
    "check:lint": "eslint types src --ext .js,.ts",
    "check:types": "tsc --noEmit",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd",
    "build:esm": "rimraf esm && tsc -outDir esm && ./scripts/build_esm_replace_imports.sh",
    "build:cjs": "rimraf cjs && tsc -outDir cjs -m CommonJS && ./scripts/build_cjs_replace_imports.sh",
    "build:umd": "rimraf umd && rollup --config rollup.ci.config.js --branch=$BUILD_BRANCH",
    "test": "npm run test:ts-decls && npm run test:e2e",
    "test:e2e": "karma start karma/e2e.karma.conf.js",
    "test:ts-decls": "tsc ts-tests/index.ts --noEmit",
    "all": "npm run check && npm run build && npm run test",
    "publish:rc": "npm run check && npm run build && npm publish --tag rc",
    "publish:stable": "npm run check && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/splitio/browser-suite.git"
  },
  "keywords": [
    "splitio",
    "browser",
    "sdk",
    "rum",
    "javascript"
  ],
  "author": "Emiliano Sanchez <emiliano.sanchez@split.io> (https://github.com/EmilianoSanchez)",
  "contributors": [
    "Nicolas Zelaya <nicolas.zelaya@split.io> (https://github.com/NicoZelaya)"
  ],
  "license": "Apache-2.0",
  "bugs": "https://github.com/splitio/browser-suite/issues",
  "homepage": "https://github.com/splitio/browser-suite#readme",
  "dependencies": {
    "@splitsoftware/browser-rum-agent": "^0.4.1",
    "@splitsoftware/splitio-browserjs": "^0.13.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.25.4",
    "fetch-mock": "^9.11.0",
    "husky": "^8.0.3",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-rollup-preprocessor": "^7.0.8",
    "karma-tap": "^4.2.0",
    "puppeteer": "^3.3.0",
    "replace": "^1.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-ts": "^3.2.0",
    "sinon": "^9.2.2",
    "tape": "4.13.2",
    "tape-catch": "1.0.6",
    "typescript": "4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run check"
    }
  },
  "sideEffects": false
}
